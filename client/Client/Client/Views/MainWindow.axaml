<Window xmlns="https://github.com/avaloniaui"        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"        xmlns:vm="using:Client.ViewModels"        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"        Width="1000"        Height="600"        x:Class="Client.Views.MainWindow"        Icon="/Assets/avalonia-logo.ico"        Title="Client">    <Design.DataContext>        <vm:MainWindowViewModel/>    </Design.DataContext>        <Grid>        <StackPanel Orientation="Horizontal" Spacing="100">            <StackPanel                 Margin="10 10 0 0"                 Spacing="10"                 Orientation="Vertical">                                                                <ComboBox HorizontalAlignment="Center" >                </ComboBox>                <ListBox Width="400" Height="450" HorizontalAlignment="Center" >                </ListBox>                                <StackPanel Orientation="Horizontal" Spacing="5">                    <TextBlock VerticalAlignment="Center">IP-адрес:</TextBlock>                    <TextBox Text="{Binding ipAddress}" Width="150" Watermark="IP">                    </TextBox>                    <Button Command="{Binding ConnectToServer}">Соединиться с сервером</Button>                </StackPanel>                                <StackPanel Orientation="Horizontal" Spacing="5">                    <Button Command="{Binding SendMessageToServer}">Передать серверу</Button>                    <Button Command="{Binding DisconnectFromServer}">Отключиться от сервера</Button>                </StackPanel>            </StackPanel>                        <StackPanel                 Margin="10 10 0 0"                 Orientation="Vertical"                 Spacing="10">                                <TextBlock HorizontalAlignment="Center">Сообщения от сервера</TextBlock>                <TextBlock Text="{Binding messageFromServer}" Width="400" Height="450"></TextBlock>            </StackPanel>        </StackPanel>    </Grid>    </Window>